<Window x:Class="Seller.App.ScanWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Seller.App"
        mc:Ignorable="d"
        Title="ScanWindow" Height="450" Width="400"
        Loaded="Window_Loaded">
    <StackPanel>
        <ComboBox x:Name="video_devices"></ComboBox>
        <Image x:Name="stream" Height="350"/>
        <TextBlock x:Name="result_l"
                   FontSize="22"/>
        <Button x:Name="start" Width="200"
                Click="start_Click">Start</Button>
    </StackPanel>
</Window>
